<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>findManegeResults</servlet-name>
		<servlet-class>manegeResults.FindResultsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findManegeResults</servlet-name>
		<url-pattern>/find</url-pattern>
	</servlet-mapping>
	
    <servlet>
    	<servlet-name>insertResults</servlet-name>
    	<servlet-class>manegeResults.ResultsInsert</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>insertResults</servlet-name>
    	<url-pattern>/insert</url-pattern>
    </servlet-mapping>

	<servlet>
		<servlet-name>updateResults</servlet-name>
		<servlet-class>manegeResults.UpdateResultsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateResults</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteResults</servlet-name>
		<servlet-class>manegeResults.DeleteResultsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteResults</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>findDelete</servlet-name>
		<servlet-class>manegeResults.findDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findDelete</servlet-name>
		<url-pattern>/remove</url-pattern>
	</servlet-mapping>	
	
	
	
	
	<servlet>
		<servlet-name>findUpdate</servlet-name>
		<servlet-class>manegeResults.findUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findUpdate</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>	
	
	
	
	
	<servlet>
		<servlet-name>AdminLogin</servlet-name>
		<servlet-class>manegeResults.AdminLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminLogin</servlet-name>
		<url-pattern>/adlogin</url-pattern>
	</servlet-mapping>	
	
	
	
	
	<servlet>
		<servlet-name>StudentLogin</servlet-name>
		<servlet-class>manegeResults.StudentLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentLogin</servlet-name>
		<url-pattern>/stlogin</url-pattern>
	</servlet-mapping>	
	
	
	
	
		<servlet>
		<servlet-name>studentHome</servlet-name>
		<servlet-class>manegeResults.HomeSearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>studentHome</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>	
	
	
	
	
	
		
		<servlet>
		<servlet-name>adminSearch</servlet-name>
		<servlet-class>manegeResults.HomeSearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>adminSearch</servlet-name>
		<url-pattern>/look</url-pattern>
	</servlet-mapping>	
	
	
	
	
	
	
	 
	
	
	
	
	

</web-app>